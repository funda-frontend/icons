{
  "name": "@funda/icons",
  "version": "1.11.0",
  "description": "funda svg icons package",
  "author": "funda",
  "keywords": [
    "svg",
    "icon"
  ],
  "repository": "github:funda-frontend/icons",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "lib/icons.js",
  "files": [
    "lib",
    "assets"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "camel-case": "^4.1.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "7.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.2.11",
    "mkdirp": "^1.0.4",
    "npm-run-all": "4.1.5",
    "semantic-release": "^17.1.1",
    "svgo": "^2.0.0",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "dev": "run-p build:* && webpack serve --mode=development",
    "build": "run-p build:* && webpack --mode=production",
    "build:esm": "node ./scripts/build.js esm",
    "build:cjs": "node ./scripts/build.js cjs",
    "build:vue": "node ./scripts/build.js vue",
    "release": "npx semantic-release",
    "minify": "svgo --config=.svgo.yml",
    "prepare": "run-p build:*",
    "prepublishOnly": "run-p build:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.svg": "npm run minify --"
  }
}
